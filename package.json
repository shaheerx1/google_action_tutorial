{
  "name": "google_actions_tutorial",
  "version": "0.0.1",
  "author": "Shaheer Majeed",
  "scripts": {
    "lint": "semistandard \"**/*.js\"",
    "start": "functions deploy google_actions_tutorial --trigger-http",
    "deploy": "gcloud beta functions deploy google_actions_tutorial --trigger-http --stage-bucket shaheer_ga_tutorial",
    "test": "npm run lint"
